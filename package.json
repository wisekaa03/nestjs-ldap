{
  "name": "nestjs-ldap",
  "version": "2.0.2",
  "description": "NestJS library to access LDAP",
  "author": "Stanislav V Vyaliy",
  "license": "MIT",
  "keywords": [
    "nestjs",
    "ldap"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/wisekaa03/nestjs-ldap.git"
  },
  "scripts": {
    "build": "rimraf ./dist && tsc -p tsconfig.json",
    "prepublish": "yarn run build",
    "publish-public": "yarn publish --access public",
    "test": "jest",
    "clean": "rm -rf node_modules yarn.lock",
    "lint": "node ./node_modules/eslint/bin/eslint.js lib/**"
  },
  "resolutions": {
    "**/**/redis-commands": "^1",
    "**/**/rimraf": "^3"
  },
  "dependencies": {
    "@nestjs/common": "*",
    "bcrypt": "*",
    "cache-manager": "^3",
    "cache-manager-ioredis": "*",
    "ldapjs": "^2",
    "nest-winston": "^1",
    "rxjs": "*",
    "url": "*",
    "winston": "^3"
  },
  "devDependencies": {
    "@types/asn1": "*",
    "@types/bcrypt": "*",
    "@types/cache-manager": "*",
    "@types/cache-manager-ioredis": "*",
    "@types/ldapjs": "*",
    "@types/node": "*",
    "@typescript-eslint/eslint-plugin": "*",
    "@typescript-eslint/parser": "*",
    "eslint": "*",
    "eslint-config-airbnb": "*",
    "eslint-config-prettier": "*",
    "eslint-import-resolver-typescript": "*",
    "eslint-plugin-import": "*",
    "eslint-plugin-jsx-a11y": "*",
    "eslint-plugin-prettier": "*",
    "eslint-plugin-react": "*",
    "eslint-plugin-react-hooks": "*",
    "jest": "^*",
    "prettier": "*",
    "reflect-metadata": "*",
    "rimraf": "*",
    "tsconfig-paths": "*",
    "typescript": "*"
  }
}
